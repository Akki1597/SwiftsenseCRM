
@{
    ViewData["Title"] = "ViewInvoice";
    Layout = "~/Views/Shared/PartialDashboardLayout.cshtml";
}

<div class="container-xl">
    <div class="row">
        @{
            Html.RenderPartial("PartialLeftMenu");
        }
        <div class="col-md-10 menuMargin" style="margin-top:30px;">
            <div class="card" style="min-height:220px;margin-top:10px;padding-left:10px;padding-right:10px">
                <div class="row">
                    <div class="col-lg-10">
                        <h4 style="margin:20px 0px 40px 20px;color:gray;">Invoice</h4>
                    </div>
                    <div class="col-lg-2">
                        <div style=" float: right; margin:20px 0px 40px 20px;"><button type="button" class="btn btnColor" onclick="location.href='@Url.Action("Billing", "Billing")'">Back</button></div>
                    </div>
                </div>

                    @{
                        Html.RenderPartial("PartialViewInvoice");
                    }

            </div>
        </div>
    </div>
</div>

